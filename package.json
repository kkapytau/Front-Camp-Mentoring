{
  "name": "front-camp-mentoring",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run run-db\" \"npm run start-server\"",
    "start-server": "nodemon ./bin/www",
    "run-db": "mongod --dbpath ./db && mongo && use blogs"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "jade": "~1.11.0",
    "mongoose": "^5.0.3",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.9.8",
    "nodemon": "^1.14.11",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.4.5",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
